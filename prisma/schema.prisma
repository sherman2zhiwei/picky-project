generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Image {
  id              Int      @id @default(autoincrement())
  originalPath    String
  compressedPath  String
  originalSize    Int
  compressedSize  Int
  mimeType        String
  width           Int
  height          Int
  filename        String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}